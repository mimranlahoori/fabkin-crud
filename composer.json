{
    "name": "lahori/fabkin-crud",
    "description": "Laravel CRUD Generator",
    "license": "MIT",
    "authors": [
        {
            "name": "M Imran Lahori",
            "email": "imranklahoori@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "crud",
        "crud generator",
        "laravel crud generator",
        "laravel package",
        "bootstrap css",
        "tailwind css",
        "alpine js",
        "livewire",
        "fabkin crud"
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Lahori\\FabkinCrud\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lahori\\FabkinCrud\\CrudServiceProvider"
            ]
        }
    },
    "minimum-stability": "@dev",
    "prefer-stable": true
}
